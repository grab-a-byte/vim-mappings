    "vim.leader": " ",
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            // Move lines down
            "before": [
                "J"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            // Move line up
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        },
        {
            //yank to Clipboard
            "before": [
                "<leader>",
                "y"
            ],
            "after": [
                "\"",
                "+",
                "y"
            ]
        },
        {
            // Paste from clipboard
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "+",
                "p"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            //Code Action
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            //Format Document
            "before": [
                "<leader>",
                "c",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "<space>"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.view.explorer",
            ]
        },
        {
            //Rename
            "before": [
                "<leader>",
                "r",
                "n"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "r"
            ],
            "commands": [
                "references-view.findReferences"
            ]
        },
        {
            "before": [
                "<leader>",
                "k"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            //Split editor sideways
            "before": [
                "<leader>",
                "|"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            //Split editor below
            "before": [
                "<leader>",
                "-"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            // Go to left window
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-W>",
                "h"
            ]
        },
        {
            // Go to lower window
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-W>",
                "j"
            ]
        },
        {
            // Go to upper window
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-W>",
                "k"
            ]
        },
        {
            // Go to right window
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-W>",
                "l"
            ]
        },
        {
            // Increase window height
            "before": [
                "<C-Up>"
            ],
            "commands": [
                "workbench.action.increaseViewHeight"
            ]
        },
        {
            // Decrease window height
            "before": [
                "<C-Up>"
            ],
            "commands": [
                "workbench.action.decreaseViewHeight"
            ]
        },
        {
            // Decrease window width
            "before": [
                "<C-left>"
            ],
            "commands": [
                "workbench.action.decreaseViewWidth"
            ]
        },
        {
            // Increase window width
            "before": [
                "<C-Right>"
            ],
            "commands": [
                "workbench.action.increaseViewWidth"
            ]
        },
        {
            // Prev buffer
            "before": [
                "<S-h>"
            ],
            "commands": [
                ":bprev"
            ]
        },
        {
            // Next buffer
            "before": [
                "<S-l>"
            ],
            "commands": [
                ":bnext"
            ]
        },
        {
            // Prev buffer
            "before": [
                "[",
                "b"
            ],
            "commands": [
                ":bprev"
            ]
        },
        {
            // Next buffer
            "before": [
                "]",
                "b"
            ],
            "commands": [
                ":bnext"
            ]
        },
        {
            // Move lines down
            "before": [
                "J"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            // Move lines up
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "s"
            ],
            "commands": [
                "breadcrumbs.focusAndSelect"
            ],
        },
        {
            // Paste from clipboard
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "+",
                "p"
            ]
        }
    ],
